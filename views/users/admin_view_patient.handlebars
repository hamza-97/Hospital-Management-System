                        <li><a href="/users/admin_book_appointment">Book Appointment</a></li>
                        <li><a href="/users/admin_manage_booked_appointments">Manage Appointment</a></li>
                    </ul>
                </li>
                <li><a class="menu-toggle"><i class="zmdi zmdi-local-hotel"></i><span>Beds</span> </a>
                    <ul class="ml-menu">
                        <li><a href="/users/admin_view_bed">Bed List</a></li>
                        <li><a href="/users/admin_add_bed">Add Bed</a></li>
                        <li><a href="/users/admin_view_category">Bed Category</a></li>
                        <li><a href="/users/admin_add_category">Add Category</a></li>  

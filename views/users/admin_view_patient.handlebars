<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2>View Patient</h2>
            <small class="text-muted">Welcome to Hospital Management System</small>
        </div>
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="header">
                        <h2>All Patients</h2>
                    </div>
                    <div class="body table-responsive">
                        <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Age</th>
                                    <th>Gender</th>
                                    <th>Phone</th>
                                    <th>Address</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>Name</th>
                                    <th>Age</th>
                                    <th>Gender</th>
                                    <th>Phone</th>
                                    <th>Address</th>
                                </tr>
                            </tfoot>
                            <tbody>
                                {{#each files}}
                                <tr>
                                    <td>{{this.name}}</td>
                              
                                    <td>{{this.age}}</td>
                                    <td>{{this.gender}}</td>               
                                    <td>{{this.phone}}</td>
                                    <td>{{this.address}}</td>
                                </tr>
                                {{/each}}
                   
                       
            
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="color-bg"></div>
